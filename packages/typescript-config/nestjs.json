{
  "extends": "./base.json",
  "compilerOptions": {
    "lib": [
      "ES2024"
    ],
    "types": [
      "node"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES2022",
    "outDir": "./dist",
    "noEmit": false,
    "strictNullChecks": true,
    "noImplicitAny": false,
    "strictBindCallApply": false
  }
}
